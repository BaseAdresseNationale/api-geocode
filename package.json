{
  "name": "@etalab/api-geocode",
  "version": "0.0.0",
  "description": "HTTP API for advanced geocoding",
  "repository": "https://github.com/etalab/api-geocode",
  "author": "Jérôme Desboeufs <jerome.desboeufs@data.gouv.fr>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "scripts": {
    "lint": "xo",
    "start": "node server"
  },
  "dependencies": {
    "addok-geocode-stream": "^0.13.0",
    "content-disposition": "^0.5.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-write-stream": "^2.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "got": "^12.5.1",
    "http-errors": "^2.0.0",
    "iconv-lite": "^0.6.3",
    "into-stream": "^7.0.0",
    "jschardet": "^3.0.0",
    "lodash-es": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pumpify": "^2.0.1"
  },
  "devDependencies": {
    "xo": "^0.52.3"
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "unicorn/prevent-abbreviations": "off",
      "unicorn/text-encoding-identifier-case": "off",
      "comma-dangle": [
        "error",
        "never"
      ]
    }
  },
  "engines": {
    "node": ">= 16"
  }
}
